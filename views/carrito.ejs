<div class="container mt-4">
    <h1 class="titulos-bold">Carrito</h1>
    <div class="row col-12 m-0 row-cols-3 mt-4">
        <% if(data[0]){ %>
        <% data.forEach(e=>{ %> 
            <div class="card col-3">
                <div class="card-body p-3">
                    <img class="w-100 mb-4" src="<%=e[2].image %>" alt="Producto"></img>
                    <h6 class="card-title"><%=e[2].title %></h6>
                    <h5 class="card-text">$ <%=e[2].price %></h5>
                    <small>Stock: <%=e[2].stock %></small>
                </div>
            </div> 
        <% }); %>
        <% } %>
    </div>
</div>
